{
  "name": "regl-scatterplot",
  "version": "0.1.0",
  "description": "WebGL-based 2D Scatterplot in Regl",
  "author": "Fritz Lekschas",
  "keywords": [
    "scatterplot",
    "2d",
    "regl",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/regl-scatterplot.js",
  "module": "src/index.js",
  "files": [
    "src/index.js",
    "dist/regl-scatterplot.js",
    "dist/regl-scatterplot.min.js"
  ],
  "scripts": {
    "build": "rollup -c; webpack --config ./webpack.config.js --mode production",
    "lint": "eslint src/index.js rollup.config.js webpack.config.js",
    "precommit": "pretty-quick --staged",
    "prepublishOnly": "npm run lint",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "watch": "rollup -cw"
  },
  "dependencies": {
    "camera-2d-simple": "^2.0.0-rc1",
    "canvas-camera-2d": "^0.5.3",
    "gl-matrix": "^2.8.1",
    "key-pressed": "flekschas/key-pressed",
    "kdbush": "^3.0.0",
    "lodash-es": "^4.17.11",
    "mouse-position": "^2.1.0",
    "mouse-pressed": "^1.0.0",
    "pub-sub-es": "^1.0.0",
    "regl": "^1.3.7",
    "regl-line": "^0.1.2",
    "scroll-speed": "flekschas/scroll-speed",
    "with-raf": "^1.0.0"
  },
  "peerDependencies": {
    "pub-sub-es": "^1.0.0",
    "regl": "^1.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^v3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-terser": "^3.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "homepage": "https://github.com/flekschas/regl-scatterplot",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/regl-scatterplot.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/regl-scatterplot/issues"
  }
}
